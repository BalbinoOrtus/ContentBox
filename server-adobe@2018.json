{
    "app":{
        "cfengine":"adobe@2018",
        "serverHomeDirectory":".engines/acf2018"
    },
    "name":"contentbox-adobe2018",
    "JVM":{
        "heapSize":"1024"
    },
    "web":{
		"directoryBrowsing" : true,
        "rewrites":{
            "enable":"true"
        },
        "http":{
            "port":"8589"
        }
	},
	"openBrowserURL" : "http://localhost:\\${http.port}",
    "trayicon":"modules_app/contentbox-custom/_content/ContentBox_300.png",
    "trayOptions":[
        {
            "label":"Open ContentBox Admin",
            "action":"openbrowser",
            "url":"http://localhost:\\${http.port}/cbadmin",
            "disabled":false,
            "image":"modules_app/contentbox-custom/_content/ContentBox-Circle_32.png"
        }
    ]
}